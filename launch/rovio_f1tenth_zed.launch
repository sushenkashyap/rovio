<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <node pkg="rovio" type="rovio_node" name="rovio" output="screen">
  <param name="filter_config" value="$(find rovio)/cfg/f1tenth_zed/f1tenth_zed_rovio.info"/>
  <param name="camera0_config" value="$(find rovio)/cfg/f1tenth_zed/f1tenth_zed_cam0.yaml"/>
  <param name="camera1_config" value="$(find rovio)/cfg/f1tenth_zed/f1tenth_zed_cam1.yaml"/>
  <remap from="cam0/image_raw" to="camera_1/left/image_raw" />
  <remap from="cam1/image_raw" to="camera_1/right/image_raw" />
  <remap from="imu0" to="imu/data" />
  </node>
</launch>